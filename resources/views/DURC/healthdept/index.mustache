			<td>{{healthdept_url}}</td>
			<td>{{wikipedia_url}}</td>
			<td>{{county_url}}</td>
			<td>{{created_at}}</td>
			<td>{{updated_at}}</td>

    </tr>
  {{/data}}

<tbody></table>



{{^is_need_paging}}
<p>

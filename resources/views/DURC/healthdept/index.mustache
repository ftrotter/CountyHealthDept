<th scope='col'>updated_at</th>

</tr></thead>
<tfoot><tr>
<th scope='col'>#</th><th scope='col'>healthdept_name</th>
<th scope='col'>county_name</th>
<th scope='col'>population</th>
<th scope='col'>state_id</th>
<th scope='col'>fips_code</th>
<th scope='col'>healthdept_url</th>
